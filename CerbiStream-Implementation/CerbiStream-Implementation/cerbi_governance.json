{
  "Version": "1.0.0",
  "LoggingProfiles": {
    "default": {
      "RequiredFields": [ "message", "timestamp" ],
      "DisallowedFields": [ "ssn", "creditCard", "npi", "privateKey", "pem" ],
      "FieldSeverities": { "password": "Forbidden", "secretValue": "Forbidden", "npi":"Forbidden", "pem":"Forbidden" },
      "SensitiveTags": [ "PII", "PHI" ]
    },
    "diagnostics": {
      "DisallowedFields": [ "ssn", "creditCard" ],
      "FieldSeverities": { "password": "Forbidden" },
      "AllowRelaxed": true
    }
  }
}
